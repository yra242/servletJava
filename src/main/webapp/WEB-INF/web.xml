<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">

  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>RedactMagazine</servlet-name>
    <servlet-class>com.example.RedactMagazine</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RedactMagazine</servlet-name>
    <url-pattern>/redactMagazine</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddToShopServlet</servlet-name>
    <servlet-class>com.example.AddToShopServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddToShopServlet</servlet-name>
    <url-pattern>/addToShop</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Shoppingcart</servlet-name>
    <servlet-class>com.example.Shoppingcart</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Shoppingcart</servlet-name>
    <url-pattern>/shoppingcart</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RemoveFromCartServlet</servlet-name>
    <servlet-class>com.example.RemoveFromCartServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>RemoveFromCartServlet</servlet-name>
    <url-pattern>/removeFromCart</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RegistrationServlet</servlet-name>
    <servlet-class>com.example.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Cabinet</servlet-name>
    <url-pattern>/cabinet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Cabinet</servlet-name>
    <servlet-class>com.example.Cabinet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegistrationServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MainEntrance</servlet-name>
    <servlet-class>com.example.MainEntrance</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.example.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Reload</servlet-name>
    <url-pattern>/reload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Reload</servlet-name>
    <servlet-class>com.example.Reload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MainEntrance</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <el-ignored>false</el-ignored>
    </jsp-property-group>
  </jsp-config>

</web-app>
